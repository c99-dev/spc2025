<div class="input-container ps-5 pe-5 m-5">
  <form class="input-group ps-5 pe-5" action="/search" method="GET">
    <input
      type="text"
      class="form-control"
      id="search"
      name="q"
      placeholder="Search by title, artist or #hashtag"
    />
    <button type="submit" class="btn btn-outline-secondary">Search</button>
  </form>
</div>
<div class="music-list-container ms-5 me-5">
  <div class="music-list ms-5 me-5">
    <% for (let i = 0; i < musicList.length; i++) { %>
    <div class="d-flex music-item border ms-5 me-5 mb-5">
      <i
        class="bi bi-file-earmark-music align-content-center ms-2 me-2"
        style="font-size: 4rem"
      ></i>
      <div>
        <h3 class="mt-3"><%= musicList[i].title %></h3>
        <p><%= musicList[i].artist %></p>
        <p>#<%= musicList[i].tags %></p>
      </div>
    </div>
    <% } %>
  </div>
</div>
